    "class Tri():\n",
    "    def __init__(self,a,b,c):\n",
    "        self.a=a\n",
    "        self.b=b\n",
    "        self.c=c\n",
    "    def area(self):\n",
    "        s=(self.a+self.b+self.c)/2\n",
    "        return (s*(s-self.a)*(s-self.b)*(s-self.c))**.5\n",
    "  \n",
    "t1 = Tri(int(input()),int(input()),int(input()))\n",
    "print(t1.area())"
   ]
